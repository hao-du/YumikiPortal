            </form>
        </div>
    </div>
    <asp:ContentPlaceHolder ID="SubContentContainer" runat="server">
    </asp:ContentPlaceHolder>
    <footer class="footer">
        <div class="container-fluid">
            <p>Yumiki 2016 - Status bar</p>
        </div>
    </footer>

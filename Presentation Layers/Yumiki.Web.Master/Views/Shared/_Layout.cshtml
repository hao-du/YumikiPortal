<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YUMIKI PORTAL</title>
    @RenderSection("HeaderContainer", required: false)

    <!--3rd Libraries-->
    <script src="../../../client/scripts/jquery.min.js"></script>
    <link href="~/client/contents/bootstrap.min.css" rel="stylesheet" />
    <script src="../../../client/scripts/bootstrap.min.js"></script>

    <!--Global Libraries-->
    <link href="~/client/contents/site.less" rel="stylesheet" />
    <script src="../../../client/scripts/site.js"></script>
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">YUMIKI PORTAL</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @{ Html.RenderAction("GetMenu", "Home"); }
                </ul>
            </div>
        </div>
    </nav>
    <div class="container-fluid">
        @RenderBody()
    </div>
    <footer class="footer">
        <div class="container-fluid">
            <p>Yumiki 2016 - Status bar</p>
        </div>
    </footer>
</body>
</html>
